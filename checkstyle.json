{
	"defineCombinations": [],
	"defaultSeverity": "INFO",
	"baseDefines": [],
	"exclude": {},
	"checks": [
		{
			"type": "Anonymous",
			"props": {}
		},
		{
			"type": "ArrayAccess",
			"props": {
				"spaceBefore": false,
				"spaceInside": false
			}
		},
		{
			"type": "ArrayLiteral",
			"props": {}
		},
		{
			"type": "AvoidInlineConditionals",
			"props": {}
		},
		{
			"type": "AvoidStarImport",
			"props": {}
		},
		{
			"type": "CatchParameterName",
			"props": {
				"format": "^(e|t|ex|[a-z][a-z][a-zA-Z]+)$"
			}
		},
		{
			"type": "ConditionalCompilation",
			"props": {
				"policy": "aligned",
				"allowSingleline": true
			}
		},
		{
			"type": "ConstantName",
			"props": {
				"ignoreExtern": true,
				"format": "^[A-Z][A-Z0-9]*(_[A-Z0-9_]+)*$",
				"tokens": []
			}
		},
		{
			"type": "CyclomaticComplexity",
			"props": {
				"thresholds": [
					{
						"complexity": 20,
						"severity": "WARNING"
					},
					{
						"complexity": 25,
						"severity": "ERROR"
					}
				]
			}
		},
		{
			"type": "DefaultComesLast",
			"props": {}
		},
		{
			"type": "Dynamic",
			"props": {}
		},
		{
			"type": "ERegLiteral",
			"props": {}
		},
		{
			"type": "EmptyBlock",
			"props": {
				"tokens": [
					"CLASS_DEF",
					"ENUM_DEF",
					"ABSTRACT_DEF",
					"TYPEDEF_DEF",
					"INTERFACE_DEF",
					"OBJECT_DECL",
					"FUNCTION",
					"FOR",
					"IF",
					"WHILE",
					"SWITCH",
					"TRY",
					"CATCH"
				],
				"option": "empty"
			}
		},
		{
			"type": "EmptyLines",
			"props": {
				"requireEmptyLineAfterPackage": true,
				"requireEmptyLineAfterInterface": true,
				"requireEmptyLineAfterAbstract": true,
				"allowEmptyLineAfterSingleLineComment": true,
				"max": 1,
				"requireEmptyLineAfterClass": true,
				"allowEmptyLineAfterMultiLineComment": true
			}
		},
		{
			"type": "EmptyPackage",
			"props": {
				"enforceEmptyPackage": false
			}
		},
		{
			"type": "FileLength",
			"props": {
				"max": 2000
			}
		},
		{
			"type": "HexadecimalLiteral",
			"props": {
				"option": "upperCase"
			}
		},
		{
			"type": "HiddenField",
			"props": {
				"ignoreSetter": true,
				"ignoreFormat": "^(main|run)$",
				"ignoreConstructorParameter": true
			}
		},
		{
			"type": "IndentationCharacter",
			"props": {
				"character": "tab",
				"ignorePattern": "^$"
			}
		},
		{
			"type": "InnerAssignment",
			"props": {
				"ignoreReturnAssignments": false
			}
		},
		{
			"type": "Interface",
			"props": {
				"allowProperties": false,
				"allowMarkerInterfaces": true
			}
		},
		{
			"type": "LeftCurly",
			"props": {
				"tokens": [
					"CLASS_DEF",
					"ENUM_DEF",
					"ABSTRACT_DEF",
					"TYPEDEF_DEF",
					"INTERFACE_DEF",
					"FUNCTION",
					"FOR",
					"IF",
					"WHILE",
					"SWITCH",
					"TRY",
					"CATCH"
				],
				"ignoreEmptySingleline": true,
				"option": "eol"
			}
		},
		{
			"type": "LineLength",
			"props": {
				"max": 160,
				"ignorePattern": "^$"
			}
		},
		{
			"type": "ListenerName",
			"props": {
				"listeners": [
					"addEventListener",
					"addListener",
					"on",
					"once"
				],
				"format": "^_?[a-z][a-zA-Z0-9]*$"
			}
		},
		{
			"type": "LocalVariableName",
			"props": {
				"ignoreExtern": true,
				"format": "^[a-z][a-zA-Z0-9]*$",
				"tokens": []
			}
		},
		{
			"type": "MagicNumber",
			"props": {
				"ignoreNumbers": [
					-1,
					0,
					1,
					2
				]
			}
		},
		{
			"type": "MemberName",
			"props": {
				"ignoreExtern": true,
				"format": "^[a-z][a-zA-Z0-9]*$",
				"tokens": []
			}
		},
		{
			"type": "MethodCount",
			"props": {
				"maxPrivate": 100,
				"maxPublic": 100,
				"maxTotal": 100
			}
		},
		{
			"type": "MethodLength",
			"props": {
				"max": 50
			}
		},
		{
			"type": "MethodName",
			"props": {
				"ignoreExtern": true,
				"format": "^[a-z][a-zA-Z0-9]*$",
				"tokens": []
			}
		},
		{
			"type": "ModifierOrder",
			"props": {
				"modifiers": [
					"MACRO",
					"OVERRIDE",
					"PUBLIC_PRIVATE",
					"STATIC",
					"INLINE",
					"DYNAMIC"
				]
			}
		},
		{
			"type": "MultipleStringLiterals",
			"props": {
				"minLength": 2,
				"ignore": "^\\s+$",
				"allowDuplicates": 2
			}
		},
		{
			"type": "MultipleVariableDeclarations",
			"props": {}
		},
		{
			"type": "NeedBraces",
			"props": {
				"allowSingleLineStatement": true,
				"tokens": [
					"FOR",
					"IF",
					"ELSE_IF",
					"WHILE",
					"DO_WHILE"
				]
			}
		},
		{
			"type": "NestedForDepth",
			"props": {
				"max": 1
			}
		},
		{
			"type": "NestedIfDepth",
			"props": {
				"max": 1
			}
		},
		{
			"type": "NestedTryDepth",
			"props": {
				"max": 1
			}
		},
		{
			"type": "NullableParameter",
			"props": {
				"option": "questionMark"
			}
		},
		{
			"type": "OperatorWhitespace",
			"props": {
				"ternaryOpPolicy": "around",
				"unaryOpPolicy": "none",
				"boolOpPolicy": "around",
				"intervalOpPolicy": "none",
				"assignOpPolicy": "around",
				"functionArgPolicy": "around",
				"bitwiseOpPolicy": "around",
				"arithmeticOpPolicy": "around",
				"compareOpPolicy": "around",
				"arrowPolicy": "around"
			}
		},
		{
			"type": "OperatorWrap",
			"props": {
				"tokens": [
					"=",
					"+",
					"-",
					"*",
					"/",
					"%",
					">",
					"<",
					">=",
					"<=",
					"==",
					"!=",
					"&",
					"|",
					"^",
					"&&",
					"||",
					"<<",
					">>",
					">>>",
					"+=",
					"-=",
					"*=",
					"/=",
					"%=",
					"<<=",
					">>=",
					">>>=",
					"|=",
					"&=",
					"^=",
					"...",
					"=>",
					"++",
					"--"
				],
				"option": "eol"
			}
		},
		{
			"type": "ParameterName",
			"props": {
				"ignoreExtern": true,
				"format": "^(_|[a-z][a-zA-Z0-9]*$)",
				"tokens": []
			}
		},
		{
			"type": "ParameterNumber",
			"props": {
				"max": 7,
				"ignoreOverriddenMethods": false
			}
		},
		{
			"type": "RedundantModifier",
			"props": {
				"enforcePrivate": false,
				"enforcePublic": false,
				"enforcePublicPrivate": false
			}
		},
		{
			"type": "Return",
			"props": {
				"allowEmptyReturn": true,
				"enforceReturnType": false
			}
		},
		{
			"type": "ReturnCount",
			"props": {
				"ignoreFormat": "^$",
				"max": 2
			}
		},
		{
			"type": "RightCurly",
			"props": {
				"tokens": [
					"CLASS_DEF",
					"ENUM_DEF",
					"ABSTRACT_DEF",
					"TYPEDEF_DEF",
					"INTERFACE_DEF",
					"OBJECT_DECL",
					"FUNCTION",
					"FOR",
					"IF",
					"WHILE",
					"SWITCH",
					"TRY",
					"CATCH"
				],
				"option": "aloneorsingle"
			}
		},
		{
			"type": "SeparatorWhitespace",
			"props": {
				"commaPolicy": "after",
				"semicolonPolicy": "after",
				"dotPolicy": "none"
			}
		},
		{
			"type": "SeparatorWrap",
			"props": {
				"tokens": [
					",",
					"."
				],
				"option": "eol"
			}
		},
		{
			"type": "SimplifyBooleanExpression",
			"props": {}
		},
		{
			"type": "SimplifyBooleanReturn",
			"props": {}
		},
		{
			"type": "Spacing",
			"props": {
				"spaceIfCondition": 0,
				"spaceAroundBinop": true,
				"spaceForLoop": 0,
				"ignoreRangeOperator": true,
				"spaceWhileLoop": 0,
				"spaceCatch": 0,
				"spaceSwitchCase": 0,
				"noSpaceAroundUnop": true
			}
		},
		{
			"type": "StringLiteral",
			"props": {
				"policy": "doubleAndInterpolation",
				"allowException": true
			}
		},
		{
			"type": "TODOComment",
			"props": {
				"format": "^\\s*(TODO|FIXME|HACK|XXX|BUG)"
			}
		},
		{
			"type": "TabForAligning",
			"props": {
				"ignorePattern": "^$"
			}
		},
		{
			"type": "Trace",
			"props": {}
		},
		{
			"type": "TrailingWhitespace",
			"props": {}
		},
		{
			"type": "Type",
			"props": {
				"ignoreEnumAbstractValues": true
			}
		},
		{
			"type": "TypeName",
			"props": {
				"ignoreExtern": true,
				"format": "^[A-Z]+[a-zA-Z0-9]*$",
				"tokens": []
			}
		},
		{
			"type": "UnnecessaryConstructor",
			"props": {}
		},
		{
			"type": "UnusedImport",
			"props": {
				"moduleTypeMap": {},
				"ignoreModules": []
			}
		},
		{
			"type": "UnusedLocalVar",
			"props": {}
		},
		{
			"type": "VariableInitialisation",
			"props": {}
		},
		{
			"type": "WhitespaceAfter",
			"props": {
				"tokens": [
					",",
					";"
				]
			}
		},
		{
			"type": "WhitespaceAround",
			"props": {
				"tokens": [
					"=",
					"+",
					"-",
					"*",
					"/",
					"%",
					">",
					"<",
					">=",
					"<=",
					"==",
					"!=",
					"&",
					"|",
					"^",
					"&&",
					"||",
					"<<",
					">>",
					">>>",
					"+=",
					"-=",
					"*=",
					"/=",
					"%=",
					"<<=",
					">>=",
					">>>=",
					"|=",
					"&=",
					"^=",
					"=>"
				]
			}
		}
	]
}